{
  "name": "nicksays",
	"displayName": "NickSays",
	"description": "Align cursors by addingg spaces, and number cursors",
	"publisher":"engineernick",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:nicksays.number_cursors",
		"onCommand:nicksays.space_align_cursors"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nicksays.number_cursors",
				"title": "NickSays: Number Cursors"
			},
			{
				"command": "nicksays.space_align_cursors",
				"title": "NickSays: Align Cursors By Adding Spaces"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
